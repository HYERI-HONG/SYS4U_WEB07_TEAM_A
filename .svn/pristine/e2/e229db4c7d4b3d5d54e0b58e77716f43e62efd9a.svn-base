@charset "UTF-8";

#wrapper{
	
	
}

#header{
	width : 100%;
	height : 77px;
	background-color : #edeee8;
}

#header_div{
	padding: 26px 20px;
	margin: 0 15%;
}

#header_logo_box{
	
	display: inline-block;
	margin-right: 25%;
}

.no_decoration_atag{
	text-decoration: none;
	margin: 0;
    padding: 0;
    border: 0;
}

#header_icon_logo{
	background-repeat: no-repeat;
	background-position: -52px -177px;
	background-image: url(../img/common/icon1.png);
    height: 24px;
    width: 24px;	
    display: inline-block;
    margin-bottom: 3px;
}
#header_icon_seperator{
	background-color: #262626;
    height: 28px;
    margin: 0 16px;
    width: 1.5px;
    display: inline-block;
}

#header_icon_brandName{
	background-repeat: no-repeat;
    background-position: 0 -146px;
    background-image: url(../img/common/icon1.png);
    height: 29px;
    width: 103px;
    display: inline-block;
}
#header_search_box{
	height: 30px;
	width: 215px;
	border: solid 1px #dbdbdb;
	background-color : white;
    padding: 3px 10px 3px 26px;
    display: inline-block;
    vertical-align: super;
   
}

#header_search_icon{
	vertical-align: baseline;
	 color: #dbdbdb;
}

#header_search_input{
    margin: 3px 0px 0px 10px;
    width: 80%;
    vertical-align: text-top;
    border: none;
    color: #dbdbdb;
    
}

#header_user_box{

	display: inline-block;
    float: right;
}

#header_user_profile{
	width: 40px;
    height: 35px;
    border-radius: 100%;
    display: inline-block;
    background-size: cover;
    border: 1px solid rgba(255,255,255,0.5);
}

#header_user_id{
	display: inline-block;
	vertical-align: top;
	margin: 5px 0px 0px 5px;
}



#content{
	
	
}

#footer{
	
	
}

#eventSource{
	display : none;
}